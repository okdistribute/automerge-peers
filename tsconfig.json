{
	"compilerOptions": {
	  "allowSyntheticDefaultImports": true,
	  "alwaysStrict": true,
	  "composite": true,
	  "declaration": true,
	  "downlevelIteration": true,
	  "esModuleInterop": true,
	  "lib": ["dom", "esnext"],
	  "module": "ESNext",
	  "moduleResolution": "node",
	  "noEmit": false,
	  "outDir": "dist",
	  "rootDir": ".",
	  "skipLibCheck": true,
	  "sourceMap": true,
	  "target": "es5",
	  "preserveSymlinks": true
	}
  }
  