{
  "compilerOptions": {
    "isolatedModules": true,
    "noImplicitAny": false,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "skipLibCheck": true,
    "strict": false,
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "es2015"
    ],
    "module": "esnext",
    "noEmit": true
  }
}
